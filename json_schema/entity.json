{
    "$schema": "http://json-schema.org/schema#",
    "version": "V0.01",
    "title": "Entity",
    "type": "object",
    "properties": {
      "EntityGUID": {
        "type": "string",
        "description": "URI or Globally Unique ID"
      },
      "Context": {
        "type": "string",
        "description": "Container location where the data is stored "
      },
      "EntityClassifcation": {
        "type": "string",
        "description": "Classifcation of the object"
    },
    "EntityClassifcationReference": {
        "type": "string",
        "description": "URL of a web service that stores the definition of the classification"
    }
  }