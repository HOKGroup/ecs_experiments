{
    "$schema": "http://json-schema.org/schema#",
    "title": "relationship.links",
    "version": "V0.01",
    "required": ["Description", "SourceEntities"],
    "type": "object",
    "properties": {
      "Description": {
        "type": "string",
        "description": "What is the relationship doing"
      },
      "AssociatedService": {
        "type": "string",
        "description": "Associated Service Attached to the relationsip"
      },
      "Service Schedule or Logic": {
        "type": "string",
        "description": "Associated Service Attached to the relationsip"
      },
      "SourceEntities": {
        "type": "array",
        "items":{
            "type":"string",
            "description": "List of Entities that start the relationship"
        }
      },
      "SourceComponents": {
        "type": "array",
        "items":{
            "type":"string",
            "description": "List of Compoennts that start the relationship"
        }
      },
      "TargetEntities": {
        "type": "array",
        "items":{
            "type":"string",
            "description": "List of Entities that recieve the relationship"
        }
      },
      "TargetComponents": {
        "type": "array",
        "items":{
            "type":"string",
            "description": "List of Components that recieve the relationship"
        }
      }
        
    }
