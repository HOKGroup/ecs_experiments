{
    "$schema": "http://json-schema.org/schema#",
    "version": "V0.01",
    "title": "Component",
    "type": "object",
    "properties": {
      "ComponentGUID": {
        "type": "string",
        "description": "URI or Globally Unique ID"
      },
      "Context": {
        "type": "string",
        "description": "Container location where the data is stored "
      },
      "EntityGUID": {
        "type": "string",
        "description": "GUID of parent entity"
    },
    "EntityClassification": {
        "type": "string",
        "description": "Classification of parent entity"
    },
    "ComponentUserName": {
        "type": "string",
        "description": "Name of the compoonent provided by user"
    },
    "ComponentUserID": {
        "type": "string",
        "description": "ID of the compoonent provided by user"
    },
    "ComponentType": {
        "type": "string",
        "description": "Type of object the component represents"
    },
    "ComponentTypeReference": {
        "type": "string",
        "description": "URL of a web service that stores the definition of the Component Types"
    },
    "ComponentVersion": {
        "type": "string",
        "description": "Version of this component"
    },
    "ComponentStatus": {
        "type": "string",
        "description": "User Defined status for component"
    },
    "AuthorSoftware": {
        "type": "string",
        "description": "Software Details of component"
    },
    "Hash1": {
        "type": "string",
        "description": "Hash of potentially many that is used to validate the object"
    },
    "PayloadDataType": {
        "type": "string",
        "description": "Known payload of data"
    },
    "Active": {
        "type": "string",
        "description": "Is the data active or not"
    },
    "DateCreated": {
        "type": "string",
        "description": "One of many potential dates"
    }
  }